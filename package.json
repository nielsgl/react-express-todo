		"test": "react-scripts test",
		"lint": "eslint .",
		"start:db": "json-server http://jsonplaceholder.typicode.com/db --port 4000"
	},
	"dependencies": {
		"prop-types": "^15.7.2",
		"ra-data-json-server": "^2.7.3",
