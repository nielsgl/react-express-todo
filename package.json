		"react-admin": "^2.7.3",
		"react-dom": "^16.8.4",
		"react-scripts": "2.1.8"
	},
	"eslintConfig": {
		"extends": "react-app",
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "error"
		},
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"node_modules",
						"src"
					]
				}
			}
		}
	},
	"eslintIgnore": [
		"node_modules",
		"build"
	],
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"directories": {
		"doc": "docs"
	},
	"devDependencies": {
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-prettier": "^3.0.1",
		"json-server": "^0.14.2",
		"prettier": "^1.16.4",
		"sqlite3": "^4.0.6"
	}
